<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd">
	
	<!-- TMBean -->
	<bean id="tmbean" class="bma.siteone.netty.thrift.hub.NettyThriftServiceHubPeer">
		<property name="entry" ref="aiThriftEntry"/>
		<!--		
		<property name="hubUrl" value="socket://127.0.0.1:9092/?module=serviceHubExt&amp;frameSize=10m"/>
		 -->
		 <property name="hubUrl" value="socket://172.19.16.195:9291/?module=serviceHubExt&amp;frameSize=10m"/>		 
		<property name="listenerUrl" value="socket://172.19.16.78:9193/?module=hubPeerListener&amp;frameSize=10m"/>
		<property name="heartbeat" value="10m"/>
		<property name="registerNode" ref="thriftServiceNode"/>
	</bean>
	<bean id="atpListener" class="bma.common.thrift.servicehub.protocol.TAIServiceHubListener.Processor">
		<constructor-arg>
			<bean class="bma.siteone.netty.thrift.hub.NettyThriftServiceHubPeer.Listener">		
				<constructor-arg ref="hubPeer"/>
			</bean>
		</constructor-arg>
	</bean>		

</beans>
